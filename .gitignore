an><br><span data-lexical-text="true">    </span><br><span data-lexical-text="true">    Flatten(),</span><br><span data-lexical-text="true">    Dense(512, activation="relu"),</span><br><span data-lexical-text="true">    Dropout(0.5),</span><br><span data-lexical-text="true">    Dense(train_generator.num_classes, activation="softmax")</span><br><span data-lexical-text="true">])</span><br><span data-lexical-text="true"></span><br><span data-lexical-text="true">cnn_model.compile(optimizer=Adam(learning_rate=0.0001),</span><br><span data-lexical-text="true">                  loss="categorical_crossentropy",</span><br><span data-lexical-text="true">                  metrics=["accuracy"])</span><br><span data-lexical-text="true"></span><br><span data-lexical-text="true">history_cnn = cnn_model.fit(train_generator,</span><br><span data-lexical-text="true">                            epochs=20,</span><br><span data-lexical-text="true">                            validation_data=val_generator)</span><br><span data-lexical-text="true"></span><br><span data-lexical-text="true"># ------------------------------</span><br><span data-lexical-text="true"># STEP 4: Model 2 - Transfer Learning with VGG16</span><br><span data-lexical-text="true"># ------------------------------</span><br><span data-lexical-text="true">base_model = VGG16(weights="imagenet", include_top=False, input_shape=(150,150,3))</span><br><span data-lexical-text="true">for layer in base_model.layers:</span><br><span data-lexical-text="true">    layer.trainable = False  # Freeze base layers</span><br><span data-lexical-text="true"></span><br><span data-lexical-text="true">vgg_model = Sequential([</span><br><span data-lexical-text="true">    base_model,</span><br><span data-lexical-text="true">    Flatten(),</span><br><span data-lexical-text="true">    Dense(512, activation="relu"),</span><br><span data-lexical-text="true">    Dropout(0.5),</span><br><span data-lexical-text="true">    Dens