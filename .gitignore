="true">from sklearn.metrics import classification_report, confusion_matrix</span><br><span data-lexical-text="true">import seaborn as sns</span><br><span data-lexical-text="true"></span><br><span data-lexical-text="true"># ------------------------------</span><br><span data-lexical-text="true"># STEP 1: Dataset Paths</span><br><span data-lexical-text="true"># ------------------------------</span><br><span data-lexical-text="true">train_dir = "dataset/train"</span><br><span data-lexical-text="true">val_dir   = "dataset/val"</span><br><span data-lexical-text="true">test_dir  = "dataset/test"</span><br><span data-lexical-text="true"></span><br><span data-lexical-text="true"># ------------------------------</span><br><span data-lexical-text="true"># STEP 2: Data Preprocessing + Augmentation</span><br><span data-lexical-text="true"># ------------------------------</span><br><span data-lexical-text="true">img_size = (150, 150)</span><br><span data-lexical-text="true">batch_size = 32</span><br><span data-lexical-text="true"></span><br><span data-lexical-text="true">train_datagen = ImageDataGenerator(</span><br><span data-lexical-text="true">    rescale=1./255,</span><br><span data-lexical-text="true">    rotation_range=10,</span><br><span data-lexical-text="true">    width_shift_range=0.02,</span><br><span data-lexical-text="true">    height_shift_range=0.02,</span><br><span data-lexical-text="true">    shear_range=0.12,</span><br><span data-lexical-text="true">    zoom_range=0.1,</span><br><span data-lexical-text="true">    horizontal_flip=True,</span><br><span data-lexical-text="true">    brightness_range=[0.85, 1.15]</span><br><span data-lexical-text="true">)</span><br><span data-lexical-text="true"></span><br><span data-lexical-text="true">val_datagen = ImageDataGenerator(rescale=1./255)</span><br><span data-lexical-text="true">test_datagen = ImageDataGenerator(rescale=1./255)</span><br><span data-lexical-text="true"></span><br><span data